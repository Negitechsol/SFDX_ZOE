    <fullName>contact_email</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK(Contact_Email__c )), NOT( REGEX( Contact_Email__c ,&quot;^[a-zA-Z0-9ñáéíóúü]+_.-]+@[a-zA-Z0-9.-]+$&quot;)))</errorConditionFormula>
    <errorDisplayField>Contact_Email__c</errorDisplayField>
    <errorMessage>Enter a valid email format</errorMessage>
</ValidationRule>
