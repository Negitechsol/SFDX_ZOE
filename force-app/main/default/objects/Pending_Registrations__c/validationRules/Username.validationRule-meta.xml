<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Username</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK(Username__c )), NOT( REGEX( Username__c  , &quot;^[a-zA-Z0-9_%^&amp;()+=!~`,.#*$?@.&amp;+-ñáéíóúü]*$&quot;)))</errorConditionFormula>
    <errorDisplayField>Username__c</errorDisplayField>
    <errorMessage>Enter a valid Username</errorMessage>
</ValidationRule>
