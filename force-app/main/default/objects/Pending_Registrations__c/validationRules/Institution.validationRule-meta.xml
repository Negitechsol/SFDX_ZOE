<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Institution</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK(Institution__c)), NOT( REGEX( Institution__c , &quot;^[a-zA-Z0-9_%^&amp;()+=!~`,.#*$?@.&amp;+-ñáéíóúü]*$&quot;)))</errorConditionFormula>
    <errorDisplayField>Institution__c</errorDisplayField>
    <errorMessage>Enter a Valid Institution</errorMessage>
</ValidationRule>
