<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Profession</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK(Profession__c)), NOT( REGEX( Profession__c, &quot;^[a-zA-Z0-9_%^&amp;()+=!~`,.#*/.\\$?@.&amp;+-ñáéíóúü]*$&quot;)))</errorConditionFormula>
    <errorDisplayField>Profession__c</errorDisplayField>
    <errorMessage>Enter a Valid Profession</errorMessage>
</ValidationRule>
