<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Nickname</fullName>
    <active>false</active>
    <errorConditionFormula>AND(NOT( ISBLANK(Nick_Name__c )), NOT( REGEX( Nick_Name__c , &quot;^[a-zA-Z0-9_%^&amp;()+=!~`,.#*$?@.&amp;+-ñáéíóúü]*$&quot;)))</errorConditionFormula>
    <errorDisplayField>Nick_Name__c</errorDisplayField>
    <errorMessage>Enter a valid Nickname</errorMessage>
</ValidationRule>
